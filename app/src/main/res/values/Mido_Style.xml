<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/myratingbar</item>
        <item name="android:minHeight">24dp</item>
        <item name="android:maxHeight">24dp</item>
    </style>
    <style name="favourite_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart" tools:targetApi="jelly_bean_mr1">true</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>

    <style name="gridVeiw">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:verticalSpacing">5dp</item>
        <item name="android:horizontalSpacing">10dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentStart" tools:targetApi="jelly_bean_mr1">true</item>
        <item name="android:layout_alignParentTop">true</item>
    </style>
    <style name="equalD">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">match_parent</item>
    </style>
</resources>